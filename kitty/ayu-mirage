color0   #1d242c
color8   #686868

# red
color1   #f07178
color9   #f07178

# green
color2   #bae67e
color10  #cbe645

# yellow
color3   #ffcc66
color11  #ffdf80

# blue
color4   #36a3d9
color12  #6871ff

# magenta
color5   #d4bfff
color13  #ff77ff

# cyan
color6   #95e6cb
color14  #a6fde1

# white
color7   #c7c7c7
