# The foreground color
foreground       #D9D7CE

# The background color
background       #212733

# The foreground for selections
selection_foreground #D9D7CE

# The background for selections
selection_background #343F4C

# The cursor color
cursor           #D9D7CE

# The color for the border of the active window
active_border_color #D9D7CE


# The color for the border of inactive windows
inactive_border_color #212733

# Tab-bar colors
active_tab_foreground #212733
active_tab_background #D9D7CE
inactive_tab_foreground #D9D7CE
inactive_tab_background #212733


# The 16 terminal colors. There are 8 basic colors, each color has a dull and
# bright version.

