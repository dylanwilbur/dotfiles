# # The foreground color
# foreground       #E6E1CF

# # The background color
# background       #0F1419

# # The foreground for selections
# selection_foreground #E6E1CF

# # The background for selections
# selection_background #253340

# # The cursor color
# cursor           #E6E1CF

# # The color for the border of the active window
# active_border_color #E6E1CF

# # The color for the border of inactive windows
# inactive_border_color #253340

# # Tab-bar colors
# active_tab_foreground #E6E1CF
# active_tab_background #253340
# inactive_tab_foreground #E6E1CF
# inactive_tab_background #0F1419


# # The 16 terminal colors. There are 8 basic colors, each color has a dull and
# # bright version.

# # black
# color0   #1d242c
# color8   #686868

# # red
# color1   #ff7733
# color9   #f07178

# # green
# color2   #c2d94c
# color10  #cbe645

# # yellow
# color3   #ffb454
# color11  #ffee99

# # blue
# color4   #50b4e6
# color12  #6871ff

# # magenta
# color5   #ca30c7
# color13  #ff77ff

# # cyan
# color6   #95e6cb
# color14  #a6fde1

# # white
# color7   #c7c7c7
# color15  #ffffff
