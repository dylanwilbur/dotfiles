{
    "auto-close-tag.activationOnLanguage": [
        "html",
        "xml",
        "php",
        "javascript",
        "javascriptreact"
    ],
    "code-runner.executorMap": {
        "ahk": "autohotkey",
        "applescript": "osascript",
        "autoit": "autoit3",
        "bat": "cmd /c",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "clojure": "lein exec",
        "coffeescript": "coffee",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "crystal": "crystal",
        "csharp": "scriptcs",
        "dart": "dart",
        "fsharp": "fsi",
        "go": "go run",
        "groovy": "groovy",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "javascript": "node",
        "julia": "julia",
        "kotlin": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
        "lua": "lua",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "ocaml": "ocaml",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "perl": "perl",
        "php": "php",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "python": "export PYTHONPATH=/usr/local/lib/python3.6/site-packages/ && python3",
        "r": "Rscript",
        "racket": "racket",
        "ruby": "ruby",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "scala": "scala",
        "shellscript": "bash",
        "swift": "swift",
        "typescript": "ts-node",
        "vbscript": "cscript //Nologo"
    },
    "code-runner.runInTerminal": false,
    "code-runner.showRunIconInEditorTitleMenu": false,
    "color-highlight.markRuler": false,
    "css.colorDecorators.enable": false,
    "editor.cursorBlinking": "smooth",
    "editor.fontFamily": "Fira Code, Roboto Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.minimap.renderCharacters": false,
    "editor.quickSuggestions": {
        "comments": true,
        "other": true,
        "strings": true
    },
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "extensions.autoUpdate": true,
    "files.associations": {
        "kwmrc": "shellscript",
        "zshrc": "shellscript",
        "antigenrc": "shellscript"
    },
    "files.autoSave": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "less.colorDecorators.enable": false,
    "material-icon-theme.showUpdateMessage": false,
    "python.formatting.formatOnSave": true,
    "python.linting.enabled": false,
    "scss.colorDecorators.enable": false,
    "swift.path.sourcekite": "/Users/splinter/.sourcekite/.build/debug/sourcekite",
    "sync.anonymousGist": false,
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.forceDownload": false,
    "sync.gist": "9115ef5de87715c9b65b703a86e2f924",
    "sync.host": "",
    "sync.lastDownload": "",
    "sync.lastUpload": "2017-06-25T17:49:29.284Z",
    "sync.pathPrefix": "",
    "sync.quietSync": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "Hyper.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.fontFamily": "",
    "terminal.integrated.fontLigatures": true,
    "typescript.check.tscVersion": false,
    "vsicons.dontShowNewVersionMessage": true,
    "vsicons.projectDetection.autoReload": true,
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Nord",
    "workbench.iconTheme": "vscode-icons",
    "workbench.sideBar.location": "left",
    "workbench.statusBar.visible": true,
    "workbench.welcome.enabled": false
}
