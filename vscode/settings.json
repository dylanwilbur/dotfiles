  "editor.renderControlCharacters": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "extensions.autoUpdate": true,
  "files.associations": {
    "antigenrc": "shellscript",
    "khdrc": "shellscript",
    "kwmrc": "shellscript",
    "zshrc": "shellscript"
  },
  "files.autoSave": "off",
